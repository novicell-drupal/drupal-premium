{
  "name": "dds-premium-frontend",
  "description": "DDS Premium frontend package.",
  "config": {
    "CSS_DRUPAL_MODULES_PATHS": "",
    "JS_DRUPAL_MODULES_PATHS": "",
    "CSS_BASE": "src/",
    "CSS_FILEMATCH": "'src/**/*.css'",
    "CSS_DIST": "dist/css",
    "WEBPACK_BASE": "src/",
    "WEBPACK_FILEMATCH": "src/**/*.js",
    "WEBPACK_DIST": "dist/js",
    "SPRITES_FILEMATCH": "assets/icons/**/**/*.svg",
    "SPRITES_DIST": "dist/icons/icons.svg",
    "IMAGES_FILEMATCH": "assets/images/**/*.{jpg,png,svg,gif}",
    "IMAGES_DIST": "dist/images",
    "FONTS_FILEMATCH": "assets/fonts/**/*",
    "FONTS_DIST": "dist/fonts",
    "CLEANUP_MATCH": "dist"
  },
  "scripts": {
    "lint": "stylelint src/**/*.css --fix",
    "cleanup": "node config/cleanup.js -i $npm_package_config_CLEANUP_MATCH",
    "fractal": "fractal start --sync",
    "fractal:build": "fractal build",
    "build:prod": "npm-run-all -l cleanup build:css build:webpack build:sprites build:fonts build:images",
    "build:dev": "npm-run-all -l cleanup build:css-dev build:webpack-dev build:sprites build:fonts build:images",
    "build:css": "cross-env NODE_ENV=production postcss $npm_package_config_CSS_FILEMATCH --dir $npm_package_config_CSS_DIST --base $npm_package_config_CSS_BASE --config config/postcss.config.js",
    "build:css-dev": "cross-env postcss $npm_package_config_CSS_FILEMATCH --dir $npm_package_config_CSS_DIST --base $npm_package_config_CSS_BASE --config config/postcss.config.js",
    "build:webpack": "fileMatch=$npm_package_config_WEBPACK_FILEMATCH webpack --config config/webpack.config.js --output-path $npm_package_config_WEBPACK_DIST --context $npm_package_config_WEBPACK_BASE --mode production",
    "build:webpack-dev": "fileMatch=$npm_package_config_WEBPACK_FILEMATCH webpack --config config/webpack.config.js --output-path $npm_package_config_WEBPACK_DIST --context $npm_package_config_WEBPACK_BASE --mode development",
    "build:css-drupal-modules": "cross-env NODE_ENV=production node config/drupal-modules.js -i $npm_package_config_CSS_DRUPAL_MODULES_PATHS --type css",
    "build:css-drupal-modules-dev": "node config/drupal-modules.js -i $npm_package_config_CSS_DRUPAL_MODULES_PATHS --type css",
    "build:js-drupal-modules": "cross-env NODE_ENV=production node config/drupal-modules.js -i $npm_package_config_JS_DRUPAL_MODULES_PATHS --type js",
    "build:js-drupal-modules-dev": "node config/drupal-modules.js -i $npm_package_config_JS_DRUPAL_MODULES_PATHS --type js",
    "build:fonts": "cross-env-shell copyfiles -u 2 $npm_package_config_FONTS_FILEMATCH $npm_package_config_FONTS_DIST -V",
    "build:sprites": "node config/sprite.js -i $npm_package_config_SPRITES_FILEMATCH -o $npm_package_config_SPRITES_DIST",
    "build:images": "node config/imagemin.js -i $npm_package_config_IMAGES_FILEMATCH -o $npm_package_config_IMAGES_DIST",
    "watch:css": "cross-env NODE_ENV=production postcss $npm_package_config_CSS_FILEMATCH --dir $npm_package_config_CSS_DIST --base $npm_package_config_CSS_BASE --config config/postcss.config.js --watch --verbose",
    "watch:css-dev": "cross-env postcss $npm_package_config_CSS_FILEMATCH --dir $npm_package_config_CSS_DIST --base $npm_package_config_CSS_BASE --config config/postcss.config.js --watch --verbose",
    "watch:webpack": "fileMatch=$npm_package_config_WEBPACK_FILEMATCH webpack --config config/webpack.config.js --output-path $npm_package_config_WEBPACK_DIST --context $npm_package_config_WEBPACK_BASE --mode production --watch",
    "watch:webpack-dev": "fileMatch=$npm_package_config_WEBPACK_FILEMATCH webpack --config config/webpack.config.js --output-path $npm_package_config_WEBPACK_DIST --context $npm_package_config_WEBPACK_BASE --mode development --watch"
  },
  "license": "MIT",
  "fractal": {
    "main": "config/fractal/fractal.config.js"
  },
  "browserslist": [
    ">= 5% in DK"
  ],
  "optionalDependencies": {
    "fsevents": "*"
  },
  "devDependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/plugin-transform-object-assign": "^7.10.4",
    "@babel/polyfill": "^7.10.4",
    "@babel/preset-env": "^7.11.5",
    "@frctl/fractal": "^1.3.0",
    "@frctl/twig": "^1.2.0",
    "babel-loader": "^8.1.0",
    "babel-polyfill": "^6.26.0",
    "caniuse-lite": "^1.0.30001151",
    "chalk": "^4.1.0",
    "copyfiles": "^2.4.0",
    "cross-env": "^7.0.2",
    "css-loader": "^5.0.0",
    "cssnano": "^4.1.10",
    "del": "^6.0.0",
    "eslint": "^7.12.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.22.1",
    "fsevents": "^2.1.3",
    "glob": "^7.1.6",
    "glob-watcher": "^5.0.5",
    "imagemin": "^7.0.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.1.4",
    "postcss-cli": "^8.0.0",
    "postcss-color-function": "^4.1.0",
    "postcss-custom-properties": "^10.0.0",
    "postcss-loader": "^4.0.3",
    "postcss-nested": "^5.0.1",
    "postcss-preset-env": "^6.7.0",
    "postcss-reporter": "^7.0.1",
    "style-loader": "^2.0.0",
    "stylelint": "^13.7.2",
    "stylelint-config-standard": "^20.0.0",
    "svg-sprite": "^1.5.0",
    "terser-webpack-plugin": "^5.0.1",
    "uglify-js": "^3.11.4",
    "webpack": "^5.2.0",
    "webpack-bundle-analyzer": "^3.9.0",
    "webpack-cli": "^4.1.0",
    "vue-loader": "^15.9.3",
    "vue-template-compiler": "^2.6.12",
    "mini-css-extract-plugin": "^1.2.0"
  },
  "dependencies": {
    "body-scroll-lock": "^3.1.5",
    "core-js": "^3.6.5",
    "lodash": "^4.17.19",
    "novicell-lazyload": "^2.2.0",
    "regenerator-runtime": "^0.13.7",
    "svg4everybody": "^2.1.9",
    "flickity": "^2.2.1",
    "flickity-as-nav-for": "^2.0.1",
    "flickity-fade": "^1.0.0",
    "hoverintent": "^2.2.1",
    "vue": "^2.6.11"
  },
  "bin": {
    "createComponent": "./config/createComponent/index.js"
  }
}
