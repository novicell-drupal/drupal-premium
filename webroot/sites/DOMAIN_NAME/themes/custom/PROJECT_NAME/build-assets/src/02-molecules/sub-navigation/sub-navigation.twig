{{ attach_library('dds_premium/sub-navigation') }}

<div class="sub-navigation">
  <ul class="sub-navigation__list">
    {%- for item in items -%}
      <li class="sub-navigation__item">
        <a href="{{ item.url }}" class="sub-navigation__link">
          {{ item.title }}
        </a>
      </li>
    {%- endfor -%}
  </ul>
</div>
