{{ attach_library('PROJECT_NAME/sub-navigation') }}

<div class="sub-navigation">
  <ul class="sub-navigation__list">
    {%- for item in items -%}
      <li class="sub-navigation__item">
        <a href="{{ item.url }}" class="sub-navigation__link">
          {{ item.title }}
          {% include '@atoms/svg-icon/svg-icon.twig' with {
            'svgIconName': 'arrow-right'
          } %}
        </a>
      </li>
    {%- endfor -%}
  </ul>
</div>
