{{ attach_library('dds_premium/inline-navigation') }}

<div{{ (not attributes ? create_attribute() : attributes).addClass('inline-navigation') }}>
  <ul class="inline-navigation__list js-inline-navigation-slider">
    {% block content %}
      {% for item in items %}
        {% include '@molecules/inline-navigation-item/inline-navigation-item.twig' with {
          'url': item.url,
          'image': item.image,
          'label': item.label,
          'inline_navigation_teaser': item.teaser
        } %}
      {% endfor %}
    {% endblock %}
  </ul>
</div>
